{
  "name": "cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@measured/puck": "^0.20.2",
    "@payloadcms/db-postgres": "3.68.3",
    "@payloadcms/next": "3.68.3",
    "@payloadcms/plugin-multi-tenant": "^3.68.3",
    "@payloadcms/plugin-nested-docs": "3.68.3",
    "@payloadcms/plugin-seo": "3.68.3",
    "@payloadcms/richtext-lexical": "3.68.3",
    "@payloadcms/ui": "3.68.3",
    "@repo/ui": "workspace:*",
    "@shopnex/payload-sdk": "^3.1.7",
    "@shopnex/sidebar-plugin": "0.0.2",
    "@tiptap/extension-color": "^3.13.0",
    "@tiptap/extension-highlight": "^3.13.0",
    "@tiptap/extension-image": "^3.13.0",
    "@tiptap/extension-text-style": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "@tiptap/starter-kit": "^3.13.0",
    "cross-env": "^10.1.0",
    "dotenv": "17.2.3",
    "graphql": "^16.12.0",
    "lucide-react": "^0.561.0",
    "next": "15.5.9",
    "payload": "3.68.3",
    "pretty": "^2.0.0",
    "rate-limiter-flexible": "^9.0.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "0.34.5",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.5.9",
    "@playwright/test": "1.57.0",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/react": "16.3.0",
    "@types/node": "^25.0.2",
    "@types/pretty": "^2.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.9",
    "jsdom": "27.3.0",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "prettier": "^3.7.4",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  }
}
